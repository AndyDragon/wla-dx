
CC = $(WLAVALGRIND) wla-gb
CFLAGS = -a -x -o
LD = $(WLAVALGRIND) wlalink
LDFLAGS = -d -v -s

SFILES = main.s
IFILES = 
OFILES = main.o

all: linked.gb

linked.gb: $(OFILES) makefile
	$(LD) $(LDFLAGS) linkfile linked.gb

main.o: main.s
	$(CC) $(CFLAGS) main.o main.s


$(OFILES): $(HFILES)


clean:
	rm -f $(OFILES) core *~ linked.gb linked.sym
