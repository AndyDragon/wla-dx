
CC = $(WLAVALGRIND) wla-65816
CFLAGS = -v -o
LD = $(WLAVALGRIND) wlalink
LDFLAGS = -v -S

SFILES = main.s
IFILES = 
OFILES = main.o

all: $(OFILES) makefile
	$(LD) $(LDFLAGS) linkfile result.rom

main.o: main.s
	$(CC) $(CFLAGS) main.o main.s


$(OFILES): $(HFILES)


clean:
	rm -f $(OFILES) core *~ result.rom *.sym
